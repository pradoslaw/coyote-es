{
  "type": "module",
  "name": "fastify",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build:ts && node -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "node --watch dist/app.js",
    "format": "npx prettier . --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^6.8.8",
    "@fastify/autoload": "^5.0.0",
    "@fastify/elasticsearch": "^3.1.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/sensible": "^5.0.0",
    "@types/elasticsearch": "^5.0.43",
    "@types/fastify-jwt": "^0.8.1",
    "dotenv": "^16.4.5",
    "elastic-builder": "^2.28.1",
    "fastify": "^4.26.1",
    "fastify-cli": "^6.1.1",
    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@types/jest": "^29.5.12",
    "c8": "^9.0.0",
    "concurrently": "^8.2.2",
    "fastify-tsconfig": "^2.0.0",
    "prettier": "3.2.5",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2",
    "jest": "^29.7.0"
  },
  "overrides": {
    "@fastify/elasticsearch": {
      "@elastic/elasticsearch": "6.8.8"
    }
  }
}
